@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix mail: <http://example.com/mail#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix ex: <http://www.example.com/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix transit: <http://vocab.org/transit/terms/> .
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix schema: <http://schema.org/>.
@prefix gn: <http://www.geonames.org/ontology#>.
@prefix geosp: <http://www.telegraphis.net/ontology/geography/geography#> .

<#InsuranceMapping>
    rml:logicalSource [
	    rml:source "hdfs://localhost:9000/thesis/insurance_test.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
	    rr:template "http://airport.example.com/{policyID}";
	    rr:class transit:Stop
    ];
    rr:predicateObjectMap [
    	rr:predicate wgs84_pos:long;
    	rr:objectMap [	rml:reference "eq_site_limit" ]
 	];
    rr:predicateObjectMap [
        rr:predicate transit:service;
        rr:objectMap [
            rr:parentTriplesMap <#M2>;
        ]
    ].